{
  "name": "nest-starter",
  "version": "1.0.0",
  "description": "Nest.js boilerplate for minimalists",
  "author": "crazyoptimist<me@crazyoptimist.net>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && node --max-old-space-size=4096 node_modules/typescript/bin/tsc",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "IS_TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d typeorm.config.ts",
    "db:migrate": "npm run typeorm migration:generate src/migrations/$npm_config_name",
    "db:upgrade": "yarn typeorm migration:run",
    "db:downgrade": "yarn typeorm migration:revert",
    "db:drop": "npm run typeorm schema:drop",
    "prepare": "husky install",
    "deploy": "npm install && npm run build && npm run start:dev",
    "update-modules": "npm i -g npm-check-updates && ncu -u && npm i"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.13.1",
    "@aws-sdk/client-cost-explorer": "^3.470.0",
    "@aws-sdk/client-dynamodb": "^3.418.0",
    "@aws-sdk/client-lambda": "^3.418.0",
    "@aws-sdk/client-sns": "^3.418.0",
    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@aws-sdk/smithy-client": "^3.374.0",
    "@nest-dynamodb/typedorm": "^1.1.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/typeorm": "^10.0.0",
    "@smithy/smithy-client": "^2.1.18",
    "@typedorm/common": "^1.15.4",
    "@typedorm/core": "^1.15.4",
    "@vendia/serverless-express": "^4.10.4",
    "aws-sdk": "^2.1464.0",
    "axios": "^1.5.1",
    "axios-retry": "^3.8.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "lint-staged": "^14.0.1",
    "lru-cache": "^10.0.1",
    "luxon": "^3.4.4",
    "module-alias": "^2.2.3",
    "mysql2": "^3.6.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.4",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "typescript-lru-cache": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@types/aws-lambda": "^8.10.122",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "@types/passport-jwt": "^3.0.10",
    "@types/source-map-support": "^0.5.8",
    "@types/supertest": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "run-script-webpack-plugin": "^0.2.0",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@app": "./dist",
    "@modules": "./dist/modules"
  }
}

{
  "name": "nest-starter",
  "version": "1.0.0",
  "description": "Nest.js boilerplate for minimalists",
  "author": "crazyoptimist<me@crazyoptimist.net>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && node --max-old-space-size=4096 node_modules/typescript/bin/tsc",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "IS_TS_NODE=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d typeorm.config.ts",
    "db:migrate": "npm run typeorm migration:generate src/migrations/$npm_config_name",
    "db:upgrade": "yarn typeorm migration:run",
    "db:downgrade": "yarn typeorm migration:revert",
    "db:drop": "npm run typeorm schema:drop",
    "prepare": "husky install",
    "deploy": "npm install && npm run build && npm run start:dev",
    "update-modules": "npm i -g npm-check-updates && ncu -u && npm i"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.18.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.521.0",
    "@aws-sdk/client-cost-explorer": "^3.521.0",
    "@aws-sdk/client-dynamodb": "^3.521.0",
    "@aws-sdk/client-lambda": "^3.521.0",
    "@aws-sdk/client-sns": "^3.521.0",
    "@aws-sdk/client-ssm": "^3.590.0",
    "@aws-sdk/lib-dynamodb": "^3.521.0",
    "@aws-sdk/smithy-client": "^3.374.0",
    "@nest-dynamodb/typedorm": "^1.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@smithy/smithy-client": "^2.4.0",
    "@typedorm/common": "^1.15.4",
    "@typedorm/core": "^1.15.4",
    "@vendia/serverless-express": "^4.12.6",
    "amazon-cognito-identity-js": "^6.3.7",
    "aws-sdk": "^2.1566.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "lint-staged": "^15.2.2",
    "lru-cache": "^10.2.0",
    "luxon": "^3.4.4",
    "module-alias": "^2.2.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "typescript-lru-cache": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/aws-lambda": "^8.10.134",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@types/passport-jwt": "^4.0.1",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "run-script-webpack-plugin": "^0.2.0",
    "supertest": "6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@app": "./dist",
    "@modules": "./dist/modules"
  }
}
